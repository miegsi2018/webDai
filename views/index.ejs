      <script>
        function datas()
        {

          var data = {


		"dataI": $('#dataI').val(),
	
		"dataF": $('#dataF').val()
          }


          $.ajax(
          {
            type: "POST",
            url: "http://localhost:8080/avgTemp",
            data: JSON.stringify(data),
            contentType: "application/json; charset=utf-8",
            crossDomain: true,
            dataType: "json",
            success: function(data, status, jqXHR)
            {

              alert("success" + data);
            },

            error: function(jqXHR, status)
            {
              console.log(jqXHR);
              alert('fail' + status.code);
            }
          });



        }
      </script>






