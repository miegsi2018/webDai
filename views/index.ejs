          align-items: stretch;
          padding: 10px 0;
        }

        .timeline__step {
          padding-right: 10px;
        }

        .timeline__step__marker {
          position: relative;
          display: table-cell;
          height: 20px;
          min-height: 20px;
          width: 20px;
          min-width: 20px;
          border: 4px solid #F2F2F2;
          -moz-border-radius: 50%;
          -webkit-border-radius: 50%;
          border-radius: 50%;
          background-color: white;
          z-index: 0;
